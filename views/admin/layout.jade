block variables
doctype html
html
  head
    title= title
    include ../partials/head
  body
    .outer-container
      .container
        if (node_env) != "production"
          .row
            h6.warning.centered DIES IST NUR EINE TESTUMGEBUNG. REGISTRIERUNGEN FUER 2016 SIND NOCH NICHT MOEGLICH
        .row
          a.four.columns.no-link(href='/admin')
            h5.centered Information
          a.four.columns.no-link(href='/admin/participants')
            h5.centered Teilnehmer
          a.four.columns.no-link(href='/logout')
            h5.centered Logout
        .row
          .offset-by-four.four.columns
            object.logo(data='/images/logo.svg', type='image/svg+xml')
      .outer-content-container
        .container
          block content
      .outer-footer-container
        .footer Footer. Info about the site. Email etc.
          p Icons by: https://linearicons.com

